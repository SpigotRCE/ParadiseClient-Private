{
  "schemaVersion": 1,
  "id": "paradiseclient-private",
  "version": "${version}",

  "name": "ParadiseClient-Private",
  "description": "The private addon for ParadiseClient",
  "authors": [],
  "contact": {},

  "license": "MIT",
  "icon": "assets/paradiseclient-private/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "io.github.spigotrce.paradiseclientprivate.client.MainClient"
    ],
    "main": ["io.github.spigotrce.paradiseclientprivate.Main"]
  },

  "mixins": [
    "paradiseclient-private.mixins.json"
    ,{
      "config": "paradiseclient-private.client.mixins.json",
      "environment": "client"
    }
  ],

  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "paradiseclient-fabric": "2.22"
  }
}
